# Frontend Environment Variables - PLACEHOLDERS
# These environment variables are used in the frontend application
# and have been set with placeholder values for local development without a backend

# API Configuration
# PLACEHOLDER: API base URL (now using mock implementations)
REACT_APP_API_BASE_URL=/api
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_TIMEOUT=10000
REACT_APP_API_VERSION=v1

# Feature Flags
# PLACEHOLDER: Enable/disable features for frontend development
REACT_APP_ENABLE_ANALYTICS_TRACKING=false
REACT_APP_ENABLE_LIVE_CHAT=false
REACT_APP_ENABLE_NOTIFICATIONS=false
REACT_APP_ENABLE_ADVANCED_ANALYTICS=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_PWA_FEATURES=true
REACT_APP_ENABLE_SERVICE_WORKER=true

# Authentication
# PLACEHOLDER: Auth-related settings - not connecting to a real backend
REACT_APP_AUTH_ENABLED=false
REACT_APP_AUTH_DOMAIN=united-tactical-defense.auth0.com
REACT_APP_AUTH_CLIENT_ID=your_client_id
REACT_APP_AUTH_REDIRECT_URI=http://localhost:3000/callback

# Map Services
# PLACEHOLDER: API keys for map services (using test keys)
REACT_APP_GOOGLE_MAPS_API_KEY=test-placeholder-key-for-maps

# Calendar Configuration
# PLACEHOLDER: Calendar service configuration
REACT_APP_MAX_BOOKING_DAYS_AHEAD=30
REACT_APP_MIN_BOOKING_HOURS_AHEAD=24
REACT_APP_ALLOW_WEEKEND_BOOKINGS=true

# External Services
# PLACEHOLDER: No actual external service connections will be made
REACT_APP_EXTERNAL_SERVICES_ENABLED=false

# TESTING-only flag to indicate we're using mock data
# PLACEHOLDER: This flag tells components we're using mocks
REACT_APP_USING_MOCK_API=true

# Performance
# PLACEHOLDER: Performance monitoring settings - disabled in mock mode
REACT_APP_PERFORMANCE_MONITORING=true

# Performance Optimization Settings
REACT_APP_ENABLE_COMPRESSION=true
REACT_APP_LAZY_LOAD_IMAGES=true
REACT_APP_PREFETCH_LINKS=true
REACT_APP_DEFER_SCRIPTS=true
GENERATE_SOURCEMAP=false
BROWSER_ARGS=--disk-cache-size=104857600 --disable-gpu-driver-bug-workarounds
REACT_APP_IMAGE_OPTIMIZATION_QUALITY=75
REACT_APP_CACHE_CONTROL_HEADER="public, max-age=31536000, immutable"

# Bundle Analysis
REACT_APP_BUNDLE_VISUALIZE=false
BUNDLE_ANALYZER=false

# Company Info
# PLACEHOLDER: Default company information for display
REACT_APP_COMPANY_NAME="United Tactical Defense"
REACT_APP_COMPANY_PHONE="(555) 123-4567"
REACT_APP_COMPANY_EMAIL="info@example.com"

# Development Environment
PORT=3000
HTTPS=false

# Miscellaneous
REACT_APP_ENVIRONMENT=production
NODE_ENV=production
REACT_APP_VERSION=$npm_package_version

# Analytics
# Google Analytics 4 Configuration
REACT_APP_GA4_MEASUREMENT_ID=G-D8Y53PKQ9P
REACT_APP_GA4_CONVERSION_ID=AW-XXXXXXXXX/XXXXXXXXXXX
REACT_APP_MEASURE_PERFORMANCE_METRICS=true
REACT_APP_MEASURE_FID=true
REACT_APP_MEASURE_LCP=true
REACT_APP_MEASURE_CLS=true
REACT_APP_MEASURE_TTI=true

# Advanced Webpack Optimizations
INLINE_RUNTIME_CHUNK=true
IMAGE_INLINE_SIZE_LIMIT=10000
REACT_APP_ENABLE_DYNAMIC_IMPORTS=true
REACT_APP_ENABLE_CODE_SPLITTING=true 