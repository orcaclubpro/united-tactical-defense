{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "axios": "^1.8.4",
    "chart.js": "^4.4.8",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "sass": "^1.86.1",
    "styled-components": "^6.1.17",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=true react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "optimize-assets": "node scripts/optimize-assets.js",
    "build:optimized": "npm run optimize-assets && npm run build:prod",
    "serve:prod": "npx serve -s build",
    "analyze:bundle": "GENERATE_SOURCEMAP=true react-scripts build && webpack-bundle-analyzer build/bundle-stats.json",
    "build:analyze": "BUNDLE_ANALYZER=true npm run build",
    "preload-critical-assets": "node scripts/critical-assets.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/react-icons": "^2.2.7",
    "compression-webpack-plugin": "^10.0.0",
    "critical": "^5.1.0",
    "critters": "^0.0.20",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^10.3.10",
    "gzip-all": "^1.0.0",
    "sharp": "^0.32.6",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
